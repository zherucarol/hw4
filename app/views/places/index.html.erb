<h1>Places I've Been</h1>
 
 <ul>
   <% for place in @places %>
    <li>
      <a href="/places/<%= place["id"] %>"><%= place["name"] %></a>
    </li>
   <% end %>
</ul>

<p><a href="/places/new">New Place</a></p>

  <form action="/places" method="GET">
    <input type="text" name="name">
    <button>Submit!</button>
  </form>